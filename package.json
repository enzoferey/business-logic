{
  "name": "business-logic",
  "version": "0.0.1",
  "description": "Decoupled, clean and well tested business logic",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzoferey/business-logic.git"
  },
  "author": "Enzo Ferey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enzoferey/business-logic/issues"
  },
  "homepage": "https://github.com/enzoferey/business-logic#readme",
  "main": "index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel modules --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/__tests__/,**/__fixtures__/\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "babel-jest": "^24.0.0",
    "jest": "^24.0.0",
    "prettier": "^1.16.1",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.4"
  }
}
